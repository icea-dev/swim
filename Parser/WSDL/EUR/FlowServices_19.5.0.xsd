	<xs:simpleType name="CountsValue">
		<xs:restriction base="xs:int">
			<xs:minInclusive value="0"/>
			<xs:maxInclusive value="100000"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="FlowId">
		<xs:restriction base="xs:string">
			<xs:pattern value="[a-zA-Z0-9_ \-\+\\\|\*=,.;:?!'@#$%^\(\)\[\]\{\}]{1,8}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="MCDMTopicId">
		<xs:restriction base="xs:string">
			<xs:pattern value="(((((H)|(M))|(F))(_))(([a-zA-Z]|[0-9])|(_)){1,300})"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="RegulationId">
		<xs:restriction base="xs:string">
			<xs:maxLength value="8"/>
			<xs:pattern value="((([A-Z]([A-Z]|[0-9]){0,5})[0-9]{2})[A-Z]{0,1})"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="RegulationIdWildcard">
		<xs:restriction base="xs:string">
			<xs:pattern value="(([A-Z]|[0-9]){1,8}|(([A-Z]|[0-9]){0,7}(\*)))"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ReroutingId">
		<xs:restriction base="xs:string">
			<xs:pattern value="(([A-Z]|[0-9])|(\*)){1,8}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ReroutingIdWildcard">
		<xs:restriction base="xs:string">
			<xs:pattern value="(([A-Z]|[0-9])|(\*)){1,8}"/>
		</xs:restriction>
	</xs:simpleType>
