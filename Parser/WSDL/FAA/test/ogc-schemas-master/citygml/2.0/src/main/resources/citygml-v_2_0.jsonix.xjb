		<jsonix:mapping package="net.opengis.citygml.vegetation.v_2_0" name="CityGML_Vegetation_2_0"/>
		<jsonix:mapping package="net.opengis.citygml.waterbody.v_2_0" name="CityGML_Waterbody_2_0"/>
		<jsonix:mapping package="net.opengis.citygml.texturedsurface.v_2_0" name="CityGML_TexturedSurface_2_0"/>

	</jaxb:bindings>
</jaxb:bindings>