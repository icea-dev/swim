		node="/xs:schema">

		<jsonix:mapping package="net.opengis.citygml.v_2_0" name="CityGML_2_0"/>
		<jsonix:mapping package="net.opengis.citygml.profiles.base.v_2_0" name="CityGML_Profiles_Base_2_0"/>
		<jsonix:mapping package="net.opengis.citygml.appearance.v_2_0" name="CityGML_Appearance_2_0"/>
		<jsonix:mapping package="net.opengis.citygml.bridge.v_2_0" name="CityGML_Bridge_2_0"/>
		<jsonix:mapping package="net.opengis.citygml.building.v_2_0" name="CityGML_Building_2_0"/>
		<jsonix:mapping package="net.opengis.citygml.cityfurniture.v_2_0" name="CityGML_CityFuntiture_2_0"/>
		<jsonix:mapping package="net.opengis.citygml.cityobjectgroup.v_2_0" name="CityGML_CityObjectGroup_2_0"/>
