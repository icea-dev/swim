		<jsonix:mapping package="net.opengis.citygml.relief.v_1_0" name="CityGML_Relief_1_0"/>
		<jsonix:mapping package="net.opengis.citygml.transportation.v_1_0" name="CityGML_Transportation_1_0"/>
		<jsonix:mapping package="net.opengis.citygml.vegetation.v_1_0" name="CityGML_Vegetation_1_0"/>
		<jsonix:mapping package="net.opengis.citygml.waterbody.v_1_0" name="CityGML_Waterbody_1_0"/>
		<jsonix:mapping package="net.opengis.citygml.texturedsurface.v_1_0" name="CityGML_TexturedSurface_1_0"/>

	</jaxb:bindings>
</jaxb:bindings>