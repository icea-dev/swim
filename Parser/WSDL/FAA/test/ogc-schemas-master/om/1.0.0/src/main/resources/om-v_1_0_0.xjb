<jaxb:bindings version="1.0" xmlns:jaxb="http://java.sun.com/xml/ns/jaxb" 
	xmlns:xs="http://www.w3.org/2001/XMLSchema" 
	xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc" 
	jaxb:extensionBindingPrefixes="xjc">

	<jaxb:bindings schemaLocation="http://schemas.opengis.net/om/1.0.0/om.xsd" node="/xs:schema">
		<jaxb:schemaBindings>
			<jaxb:package name="net.opengis.om.v_1_0_0"/>
		</jaxb:schemaBindings>
	</jaxb:bindings>
	<jaxb:bindings schemaLocation="http://schemas.opengis.net/om/1.0.0/observation.xsd" node="/xs:schema">
		<jaxb:bindings node="xs:complexType[@name='ProcessPropertyType']//xs:element[@ref='sml:_Process']">
			<jaxb:property name="SensorMLProcess"/>
		</jaxb:bindings>
	</jaxb:bindings>
</jaxb:bindings>