							<appinfo>
								<gml:description>AVGAS [Octane 100 aviation gasoline.] </gml:description>
							</appinfo>
						</annotation>
					</enumeration>
					<enumeration value="AVGAS_LL">
						<annotation>
							<appinfo>
								<gml:description>AVGAS-LL [Octane 100 Low Lead aviation gasoline.]</gml:description>
							</appinfo>
						</annotation>
					</enumeration>
					<enumeration value="OCT73">
						<annotation>
							<appinfo>
								<gml:description>OCT73 [Octane 73 aviation gasoline.] </gml:description>
							</appinfo>
						</annotation>
					</enumeration>
					<enumeration value="OCT80">
						<annotation>
							<appinfo>
								<gml:description>OCT80 [Octane 80 aviation gasoline.] </gml:description>
							</appinfo>
						</annotation>
					</enumeration>
					<enumeration value="OCT82UL">
