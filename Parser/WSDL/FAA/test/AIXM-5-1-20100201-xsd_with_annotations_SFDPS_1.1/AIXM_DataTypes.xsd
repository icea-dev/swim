							<appinfo>
								<gml:description>OCT82UL [Octane 82 low-octane unleaded aviation gasoline.]</gml:description>
							</appinfo>
						</annotation>
					</enumeration>
					<enumeration value="OCT80_87">
						<annotation>
							<appinfo>
								<gml:description>OCT80-87 [Octane 80-87 aviation gasoline.] </gml:description>
							</appinfo>
						</annotation>
					</enumeration>
					<enumeration value="OCT91_98">
						<annotation>
							<appinfo>
								<gml:description>OCT91-98 [Octane 91-98 aviation gasoline.] </gml:description>
							</appinfo>
						</annotation>
					</enumeration>
					<enumeration value="OCT100_130">
						<annotation>
