			<extension base="aixm:AbstractAIXMPropertyType">
				<sequence>
					<element ref="aixm:ObstacleAssessmentArea"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<element name="ObstacleAssessmentArea" type="aixm:ObstacleAssessmentAreaType">
		<annotation>
			<appinfo>
				<gml:description>The geometry of surfaces used to define the altitude the length of a segment, holding pattern, etc., in function of the obstructions located in the area.</gml:description>
			</appinfo>
		</annotation>
	</element>
	<complexType name="ObstacleAssessmentAreaType">
		<complexContent>
			<extension base="aixm:AbstractAIXMObjectType">
				<sequence>
					<group ref="aixm:ObstacleAssessmentAreaPropertyGroup"/>
					<element name="extension" minOccurs="0" maxOccurs="unbounded">
						<complexType>
							<sequence>
								<element ref="aixm:AbstractObstacleAssessmentAreaExtension"/>
							</sequence>
							<attributeGroup ref="gml:OwnershipAttributeGroup"/>
						</complexType>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<group name="ObstacleAssessmentAreaPropertyGroup">
		<sequence>
			<element name="type" type="aixm:CodeObstacleAssessmentSurfaceType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>Indicated the type of area being assessed  such as 40:1, 72:1, MA, Final, etc</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="sectionNumber" type="aixm:NoNumberType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>a sequence number indicating a part of an assessment area.</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="slope" type="aixm:ValSlopeType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>The slope value of the extent</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="assessedAltitude" type="aixm:ValDistanceVerticalType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>Highest of the minimum altitudes in controlling obstacles plus the altitude adjustments (operational factors)</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="slopeLowerAltitude" type="aixm:ValDistanceVerticalType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>The altitude at the bottom of the assessment area</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="gradientLowHigh" type="aixm:ValSlopeType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>Low to high gradient within the airspace</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="surfaceZone" type="aixm:CodeObstructionIdSurfaceZoneType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>Specifies zones within obstruction identification surfaces (OIS)</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="safetyRegulation" type="aixm:TextNameType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>Identifier for the safety regulations in effect within the zone</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="aircraftCategory" type="aixm:AircraftCharacteristicPropertyType" nillable="true" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<appinfo>
						<gml:description>Classification of aircraft based on 1.3 times stall speed in landing configuration at maximum certified landing mass.</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="significantObstacle" type="aixm:ObstructionPropertyType" nillable="true" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<appinfo>
						<gml:description>The obstacle that controls the altitude definition of the assessment area would penetrate the 40:1 OCS</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="surface" type="aixm:SurfacePropertyType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>The geographical representation of the Assessment Area.</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="startingCurve" type="aixm:CurvePropertyType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>The curve that starts the ObstacleAssessmentArea.</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="annotation" type="aixm:NotePropertyType" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
