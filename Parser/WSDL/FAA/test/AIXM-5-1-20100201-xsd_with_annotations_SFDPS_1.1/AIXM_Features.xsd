						<gml:description>The general cardinal direction of the Aerial Refuelling procedure, as a category.</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="name" type="aixm:TextNameType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>A free text identifier by which the Aerial Refuelling route is known.</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="type" type="aixm:CodeAerialRefuellingType" nillable="true" minOccurs="0">
