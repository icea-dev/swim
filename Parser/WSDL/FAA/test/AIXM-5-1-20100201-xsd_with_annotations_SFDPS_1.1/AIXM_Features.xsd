			</element>
			<element name="annotation" type="aixm:NotePropertyType" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
			<element name="theUnit" type="aixm:UnitPropertyType" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>The gorund station where the Radar Scope is utilized.</gml:description>
					</appinfo>
				</annotation>
			</element>
		</sequence>
	</group>
	<element name="AbstractSurveillanceGroundStationExtension" type="aixm:AbstractExtensionType" abstract="true" substitutionGroup="aixm:AbstractExtension"/>
	<complexType name="ObstacleAssessmentAreaPropertyType">
