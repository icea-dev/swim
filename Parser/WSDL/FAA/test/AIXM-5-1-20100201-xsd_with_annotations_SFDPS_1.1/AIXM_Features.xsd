				<annotation>
					<appinfo>
						<gml:description>Indicates if the reflector is for Touchdown or a Runway End.</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="touchdownReflector" type="aixm:ElevatedPointPropertyType" nillable="true" minOccurs="0">
				<annotation>
