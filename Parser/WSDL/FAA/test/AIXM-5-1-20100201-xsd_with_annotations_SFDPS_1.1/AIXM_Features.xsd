				<annotation>
					<appinfo>
						<gml:description>Ability to adjust the segment altitude</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="primaryAlternateMinimum" type="aixm:CodeYesNoType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>Adjustments applicable to either local altimeter or secondary altimeter. Applicable to the final minimums based on altimeter settings. Set to Y if primary altimeter.  Not applicable for TAA or MSA.</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="altitudeAdjustment" type="aixm:ValDistanceVerticalType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>The adjustment value</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="localRemoteCode" type="aixm:CodeYesNoType" nillable="true" minOccurs="0"/>
			<element name="annotation" type="aixm:NotePropertyType" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
	</group>
