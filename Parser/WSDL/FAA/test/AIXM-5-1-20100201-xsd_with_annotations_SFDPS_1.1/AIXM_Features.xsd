					<appinfo>
						<gml:description>The definition of the AerialRefuelling anchor.</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="oppositeTrack" type="aixm:AerialRefuellingPropertyType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>The opposite AerialRefuelling.</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="managingOrganisation" type="aixm:AuthorityForAerialRefuellingPropertyType" nillable="true" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<appinfo>
						<gml:description>The OrganisationAuthority that manages the AerialRefuelling.</gml:description>
					</appinfo>
