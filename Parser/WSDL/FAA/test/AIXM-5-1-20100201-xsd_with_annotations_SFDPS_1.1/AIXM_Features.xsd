	</complexType>
	<group name="SegmentPointPropertyGroup">
		<sequence>
			<element name="reportingATC" type="aixm:CodeATCReportingType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>Indicating the type of position report required by an ATC Unit.  Eg.: compulsory or on request.</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="flyOver" type="aixm:CodeYesNoType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>Indicates if the aircraft is required to fly directly over the fix. If code is Yes, then it is a 'fly-over' waypoint. If the code is No, then the associated fix is a 'fly-by' waypoint. [ICAO] Waypoints are identified as either flyover or fly-by.

Fly-by waypoint. A waypoint which requires turn anticipation to allow tangential interception of the next segment of a route or procedure

Flyover waypoint. A waypoint at which a turn is initiated in order to join the next segment of a route or procedure.

</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="waypoint" type="aixm:CodeYesNoType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>A point used for RNAV procedures/routes.</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="radarGuidance" type="aixm:CodeYesNoType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>Radar guidance is possible for reaching this point.</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="facilityMakeup" type="aixm:PointReferencePropertyType" nillable="true" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<appinfo>
						<gml:description>Specific facility make up used by aircraft to locate the segment point.</gml:description>
					</appinfo>
				</annotation>
			</element>
			<choice>
				<element name="pointChoice_fixDesignatedPoint" type="aixm:DesignatedPointPropertyType" nillable="true" minOccurs="0">
					<annotation>
						<appinfo>
							<gml:description>A selected point that may have references (distance and/or angle indications)</gml:description>
						</appinfo>
					</annotation>
				</element>
				<element name="pointChoice_navaidSystem" type="aixm:NavaidPropertyType" nillable="true" minOccurs="0">
					<annotation>
						<appinfo>
							<gml:description>A navaid used as significant point.</gml:description>
						</appinfo>
					</annotation>
				</element>
				<element name="pointChoice_aimingPoint" type="aixm:TouchDownLiftOffPropertyType" nillable="true" minOccurs="0">
					<annotation>
						<appinfo>
							<gml:description>The point at the centre of the TLOF.</gml:description>
						</appinfo>
					</annotation>
				</element>
				<element name="pointChoice_runwayPoint" type="aixm:RunwayCentrelinePointPropertyType" nillable="true" minOccurs="0">
					<annotation>
						<appinfo>
							<gml:description>A designated point on the runway centreline.</gml:description>
						</appinfo>
					</annotation>
				</element>
				<element name="pointChoice_airportReferencePoint" type="aixm:AirportHeliportPropertyType" nillable="true" minOccurs="0">
					<annotation>
						<appinfo>
							<gml:description>the point designated as the center of the airport.</gml:description>
						</appinfo>
					</annotation>
				</element>
				<element name="pointChoice_position" type="aixm:PointPropertyType" nillable="true" minOccurs="0">
					<annotation>
						<appinfo>
							<gml:description>An unnamed point designated only with a latitude/longitude.</gml:description>
						</appinfo>
					</annotation>
				</element>
			</choice>
			<element name="extendedServiceVolume" type="aixm:RadioFrequencyAreaPropertyType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>An approved and flight checked extension to the standard service volume of a navaid</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="annotation" type="aixm:NotePropertyType" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
