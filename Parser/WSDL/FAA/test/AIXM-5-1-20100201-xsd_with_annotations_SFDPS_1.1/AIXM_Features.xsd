			<extension base="aixm:AbstractAIXMObjectType">
				<sequence>
					<group ref="aixm:FlightCharacteristicPropertyGroup"/>
					<element name="extension" minOccurs="0" maxOccurs="unbounded">
						<complexType>
							<sequence>
								<element ref="aixm:AbstractFlightCharacteristicExtension"/>
							</sequence>
							<attributeGroup ref="gml:OwnershipAttributeGroup"/>
						</complexType>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<group name="FlightCharacteristicPropertyGroup">
		<sequence>
			<element name="type" type="aixm:CodeFlightType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>A code indicating a specified  type of a flight, such as OAT/GAT.</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="rule" type="aixm:CodeFlightRuleType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>A code indicating a specific flight rule, such as IFR or VFR.</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="status" type="aixm:CodeFlightStatusType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>The designation of a special status for a flight (e.g. ""HEAD"" for Head of State, ""STATE"" for State aircraft other than that of the Head of State).</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="military" type="aixm:CodeMilitaryStatusType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>A code indicating whether it is a military flight.</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="origin" type="aixm:CodeFlightOriginType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>A code indicating the origin of the flight with regard to the national borders.</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="purpose" type="aixm:CodeFlightPurposeType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>A code indicating the purpose of the flight.</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="annotation" type="aixm:NotePropertyType" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
	</group>
	<element name="AbstractFlightCharacteristicExtension" type="aixm:AbstractExtensionType" abstract="true" substitutionGroup="aixm:AbstractExtension"/>
	<complexType name="LightElementPropertyType">
