	</complexType>
	<group name="MarkerBeaconPropertyGroup">
		<sequence>
			<group ref="aixm:NavaidEquipmentPropertyGroup" minOccurs="0"/>
			<element name="class" type="aixm:CodeMarkerBeaconSignalType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>A code indicating the class of the radio marker.
For example, fan marker, low-powered fan marker, Z marker.
</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="frequency" type="aixm:ValFrequencyType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>The value of the radio emission frequency.
</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="axisBearing" type="aixm:ValBearingType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>The true bearing of the minor axis of the marker beacon.
(source: ARINC 424 Specification)
</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="auralMorseCode" type="aixm:CodeAuralMorseType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>The Morse code transmitted by the beacon.</gml:description>
					</appinfo>
				</annotation>
			</element>
		</sequence>
	</group>
	<element name="AbstractMarkerBeaconExtension" type="aixm:AbstractExtensionType" abstract="true" substitutionGroup="aixm:AbstractExtension"/>
	<complexType name="NavaidPropertyType">
		<attributeGroup ref="gml:OwnershipAttributeGroup"/>
		<attributeGroup ref="gml:AssociationAttributeGroup"/>
	</complexType>
	<element name="Navaid" type="aixm:NavaidType" substitutionGroup="aixm:AbstractAIXMFeature">
		<annotation>
			<appinfo>
				<gml:description>A service providing guidance information or position data for the efficient and safe operation of aircraft supported by one or more radio navigation aids.</gml:description>
			</appinfo>
		</annotation>
	</element>
	<complexType name="NavaidType">
		<complexContent>
			<extension base="aixm:AbstractAIXMFeatureType">
				<sequence>
					<element name="timeSlice" type="aixm:NavaidTimeSlicePropertyType" maxOccurs="unbounded"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="NavaidTimeSlicePropertyType">
		<sequence>
			<element ref="aixm:NavaidTimeSlice"/>
		</sequence>
		<attributeGroup ref="gml:OwnershipAttributeGroup"/>
	</complexType>
	<element name="NavaidTimeSlice" type="aixm:NavaidTimeSliceType" substitutionGroup="gml:AbstractTimeSlice"/>
	<complexType name="NavaidTimeSliceType">
		<complexContent>
			<extension base="aixm:AbstractAIXMTimeSliceType">
				<sequence>
					<group ref="aixm:NavaidPropertyGroup"/>
					<element name="extension" minOccurs="0" maxOccurs="unbounded">
						<complexType>
							<sequence>
								<element ref="aixm:AbstractNavaidExtension"/>
							</sequence>
							<attributeGroup ref="gml:OwnershipAttributeGroup"/>
						</complexType>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<group name="NavaidPropertyGroup">
		<sequence>
			<element name="type" type="aixm:CodeNavaidServiceType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>Type of the navaid service such as ILS, MLS, VORTAC, VOR/DME, etc.</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="designator" type="aixm:CodeNavaidDesignatorType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>The coded identifier given to the navaid system.</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="name" type="aixm:TextNameType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>The long name given to the composite navaid.</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="flightChecked" type="aixm:CodeYesNoType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>Indicates if the navaid has been flight checked.</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="purpose" type="aixm:CodeNavaidPurposeType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>Indication of the intended usage of the navaid (enroute or terminal)</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="signalPerformance" type="aixm:CodeSignalPerformanceILSType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>The signal performance level indicating the precision of an ILS or MLS system.</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="courseQuality" type="aixm:CodeCourseQualityILSType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>An indication of the signal quality of the ILS course structure based on the location the precision signal is usable to.</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="integrityLevel" type="aixm:CodeIntegrityLevelILSType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>That quality which relates to the trust which can be placed in the correctness of the information supplied by the ILS facility.</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="touchDownLiftOff" type="aixm:TouchDownLiftOffPropertyType" nillable="true" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<appinfo>
						<gml:description>The touch down or lift-off area for helicopters.
</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="navaidEquipment" type="aixm:NavaidComponentPropertyType" nillable="true" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<appinfo>
						<gml:description>the component of the navaid system</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="location" type="aixm:ElevatedPointPropertyType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>Graphical location of the navaid (system).  This should be the same location as the equipment that provides navigability</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="runwayDirection" type="aixm:RunwayDirectionPropertyType" nillable="true" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<appinfo>
						<gml:description>The landing and take-off directions of a runway</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="servedAirport" type="aixm:AirportHeliportPropertyType" nillable="true" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<appinfo>
						<gml:description>The AirportHeliport served by the Navaid.</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="availability" type="aixm:NavaidOperationalStatusPropertyType" nillable="true" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<appinfo>
						<gml:description>The operational status of the navaid.</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="annotation" type="aixm:NotePropertyType" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
	</group>
	<element name="AbstractNavaidExtension" type="aixm:AbstractExtensionType" abstract="true" substitutionGroup="aixm:AbstractExtension"/>
