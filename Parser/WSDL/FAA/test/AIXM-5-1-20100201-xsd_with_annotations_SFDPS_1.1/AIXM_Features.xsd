		<attributeGroup ref="gml:AssociationAttributeGroup"/>
	</complexType>
	<element name="AbstractAirportHeliportProtectionArea" type="aixm:AbstractAirportHeliportProtectionAreaType" abstract="true" substitutionGroup="aixm:AbstractAIXMFeature">
		<annotation>
			<appinfo>
				<gml:description>An area situated in the vicinity of a runway, FATO or TLOF, provided to protect aircraft during manoeuvring, take-off and/or landing operations.</gml:description>
			</appinfo>
		</annotation>
	</element>
	<complexType name="AbstractAirportHeliportProtectionAreaType" abstract="true">
		<complexContent>
			<extension base="aixm:AbstractAIXMFeatureType"/>
		</complexContent>
	</complexType>
	<group name="AirportHeliportProtectionAreaPropertyGroup">
		<sequence>
			<element name="width" type="aixm:ValDistanceType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>The value of the physical width of the protection area.</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="length" type="aixm:ValDistanceType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>The value of the physical length of the protection area.</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="lighting" type="aixm:CodeYesNoType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>The availability of a lighting system that visually identified the Protection Area in low visibility conditions.</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="obstacleFree" type="aixm:CodeYesNoType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>Indicates if the protection area is obstacle free.</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="surfaceProperties" type="aixm:SurfaceCharacteristicsPropertyType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>The surface characteristics of the AirportHeliportProtectionArea.</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="extent" type="aixm:ElevatedSurfacePropertyType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>Extent of the protection area.</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="annotation" type="aixm:NotePropertyType" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
	</group>
	<element name="AbstractAirportHeliportProtectionAreaExtension" type="aixm:AbstractExtensionType" abstract="true" substitutionGroup="aixm:AbstractExtension"/>
	<complexType name="AirportHeliportCollocationPropertyType">
		<attributeGroup ref="gml:OwnershipAttributeGroup"/>
		<attributeGroup ref="gml:AssociationAttributeGroup"/>
	</complexType>
	<element name="AirportHeliportCollocation" type="aixm:AirportHeliportCollocationType" substitutionGroup="aixm:AbstractAIXMFeature">
		<annotation>
			<appinfo>
				<gml:description>Two aerodromes/heliports may be co-located sharing some or all of their ground facilities.
E.g. a civil and a military aerodrome using the same runway.</gml:description>
			</appinfo>
		</annotation>
	</element>
	<complexType name="AirportHeliportCollocationType">
		<complexContent>
			<extension base="aixm:AbstractAIXMFeatureType">
				<sequence>
					<element name="timeSlice" type="aixm:AirportHeliportCollocationTimeSlicePropertyType" maxOccurs="unbounded"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="AirportHeliportCollocationTimeSlicePropertyType">
		<sequence>
			<element ref="aixm:AirportHeliportCollocationTimeSlice"/>
		</sequence>
		<attributeGroup ref="gml:OwnershipAttributeGroup"/>
	</complexType>
	<element name="AirportHeliportCollocationTimeSlice" type="aixm:AirportHeliportCollocationTimeSliceType" substitutionGroup="gml:AbstractTimeSlice"/>
	<complexType name="AirportHeliportCollocationTimeSliceType">
		<complexContent>
			<extension base="aixm:AbstractAIXMTimeSliceType">
				<sequence>
					<group ref="aixm:AirportHeliportCollocationPropertyGroup"/>
					<element name="extension" minOccurs="0" maxOccurs="unbounded">
						<complexType>
							<sequence>
								<element ref="aixm:AbstractAirportHeliportCollocationExtension"/>
							</sequence>
							<attributeGroup ref="gml:OwnershipAttributeGroup"/>
						</complexType>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<group name="AirportHeliportCollocationPropertyGroup">
		<sequence>
			<element name="type" type="aixm:CodeAirportHeliportCollocationType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>A code indicating the extent of the collocation situation of the two aerodrome/heliports.</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="hostAirport" type="aixm:AirportHeliportPropertyType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>The host Airport.</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="dependentAirport" type="aixm:AirportHeliportPropertyType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>The airport that is colocated at the host Airport.</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="annotation" type="aixm:NotePropertyType" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
	</group>
	<element name="AbstractAirportHeliportCollocationExtension" type="aixm:AbstractExtensionType" abstract="true" substitutionGroup="aixm:AbstractExtension"/>
	<complexType name="AirportHeliportPropertyType">
		<attributeGroup ref="gml:OwnershipAttributeGroup"/>
