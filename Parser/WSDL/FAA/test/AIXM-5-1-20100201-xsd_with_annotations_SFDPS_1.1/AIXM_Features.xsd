		<complexContent>
			<extension base="aixm:AbstractNavaidEquipmentType">
				<sequence>
					<element name="timeSlice" type="aixm:MarkerBeaconTimeSlicePropertyType" maxOccurs="unbounded"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="MarkerBeaconTimeSlicePropertyType">
		<sequence>
			<element ref="aixm:MarkerBeaconTimeSlice"/>
		</sequence>
		<attributeGroup ref="gml:OwnershipAttributeGroup"/>
	</complexType>
	<element name="MarkerBeaconTimeSlice" type="aixm:MarkerBeaconTimeSliceType" substitutionGroup="gml:AbstractTimeSlice"/>
