					<appinfo>
						<gml:description>The set of rules followed when encoding the procedure legs</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="flightChecked" type="aixm:CodeYesNoType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>Indicates if the procedure has been flight checked.</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="name" type="aixm:TextNameType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>The name of the instrument procedure.</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="RNAV" type="aixm:CodeYesNoType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>Indicates the procedure is an RNAV procedure</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="availability" type="aixm:ProcedureAvailabilityPropertyType" nillable="true" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<appinfo>
						<gml:description>The operational availability of the Procedure.</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="airportHeliport" type="aixm:AirportHeliportPropertyType" nillable="true" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<appinfo>
						<gml:description>The area on land or water (including any buildings, installations and equipment) intended to be used for the arrival, departure and surface movement of aircraft/helicopters.
</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="aircraftCharacteristic" type="aixm:AircraftCharacteristicPropertyType" nillable="true" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<appinfo>
						<gml:description>aircraft equipment or category limits</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="flightTransition" type="aixm:ProcedureTransitionPropertyType" nillable="true" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<appinfo>
						<gml:description>A branch  on an approach procedure, SID or STAR.
</gml:description>
					</appinfo>
				</annotation>
			</element>
			<choice>
				<element name="guidanceFacility_navaid" type="aixm:NavaidPropertyType" nillable="true" minOccurs="0">
					<annotation>
						<appinfo>
							<gml:description>The associated Navaid.</gml:description>
						</appinfo>
					</annotation>
				</element>
				<element name="guidanceFacility_specialNavigationSystem" type="aixm:SpecialNavigationSystemPropertyType" nillable="true" minOccurs="0">
					<annotation>
						<appinfo>
							<gml:description>The navigation system.</gml:description>
						</appinfo>
					</annotation>
				</element>
				<element name="guidanceFacility_radar" type="aixm:RadarSystemPropertyType" nillable="true" minOccurs="0">
					<annotation>
						<appinfo>
							<gml:description>The Radar System.</gml:description>
						</appinfo>
					</annotation>
				</element>
			</choice>
			<element name="annotation" type="aixm:NotePropertyType" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
			<element name="safeAltitude" type="aixm:SafeAltitudeAreaPropertyType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>A series of minimum altitudes in the area where the procedure is executed.</gml:description>
					</appinfo>
				</annotation>
			</element>
		</sequence>
	</group>
	<element name="AbstractProcedureExtension" type="aixm:AbstractExtensionType" abstract="true" substitutionGroup="aixm:AbstractExtension"/>
	<complexType name="NavigationAreaRestrictionPropertyType">
		<attributeGroup ref="gml:OwnershipAttributeGroup"/>
		<attributeGroup ref="gml:AssociationAttributeGroup"/>
	</complexType>
	<element name="NavigationAreaRestriction" type="aixm:NavigationAreaRestrictionType" substitutionGroup="aixm:AbstractAIXMFeature">
		<annotation>
			<appinfo>
				<gml:description>Areas that are restricted from use for a procedure.  Example is restricted areas defined for an omnidirectional departure.</gml:description>
			</appinfo>
		</annotation>
	</element>
	<complexType name="NavigationAreaRestrictionType">
