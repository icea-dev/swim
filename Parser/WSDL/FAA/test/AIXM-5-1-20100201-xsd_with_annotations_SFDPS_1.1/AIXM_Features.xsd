			<element name="speed" type="aixm:ValSpeedType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>A speed (IAS) that can be maintained by the aircraft.</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="speedInterpretation" type="aixm:CodeValueInterpretationType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>Indicates whether the speed is a maximum or minimum imposed value.</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="wakeTurbulence" type="aixm:CodeWakeTurbulenceType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>The wake turbulence category of the aircraft.</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="navigationEquipment" type="aixm:CodeNavigationEquipmentType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>An indication of the aircraft capability to use a certain ground based, satellite based or on-board system for aerial navigation.</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="navigationSpecification" type="aixm:CodeNavigationSpecificationType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>A set of aircraft and flight crew requirements needed to support performance-based navigation operations.</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="verticalSeparationCapability" type="aixm:CodeRVSMType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>An indication of the aircraft capability to maintain a specified vertical separation.</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="antiCollisionAndSeparationEquipment" type="aixm:CodeEquipmentAntiCollisionType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>An indication of the aircraft equipment with collision avoidance systems.</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="communicationEquipment" type="aixm:CodeCommunicationModeType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>An indication of the aircraft equipment with voice and data communication devices.</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="surveillanceEquipment" type="aixm:CodeTransponderType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>An indication of the aircraft capability for operating with a certain surveillance system.</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="annotation" type="aixm:NotePropertyType" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
	</group>
	<element name="AbstractAircraftCharacteristicExtension" type="aixm:AbstractExtensionType" abstract="true" substitutionGroup="aixm:AbstractExtension"/>
