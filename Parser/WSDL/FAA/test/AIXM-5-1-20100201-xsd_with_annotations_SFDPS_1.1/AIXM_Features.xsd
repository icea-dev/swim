</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="designCriteria" type="aixm:CodeDesignStandardType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>Rules used in the design of the procedure.
</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="codingStandard" type="aixm:CodeProcedureCodingStandardType" nillable="true" minOccurs="0">
