					<appinfo>
						<gml:description>The definition of the AerialRefuellingTrack</gml:description>
					</appinfo>
				</annotation>
			</element>
			<element name="anchor" type="aixm:AerialRefuellingAnchorPropertyType" nillable="true" minOccurs="0" maxOccurs="unbounded">
